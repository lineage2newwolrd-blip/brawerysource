<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!--
    [string]
        value: A text to be displayed
        color: Color if ARGB hex format
        size: A font size
        family: Font family (i.e. Tahoma, Arial, ...)
        pos: TopRight, TopLeft, TopCenter, MiddleRight, MiddleLeft, MiddleCenter, BottomRight, BottomLeft, BottomCenter
        style:
            Normal - normal text
            Shadowed - text with a shadow

        msgId:
            None - no special value (default)
            FPS - inserts current screen refrash rate insted of %d in the text
            Time - inserts current time instead of %s

        showtime: Number of milliseconds the text will be displayed. Remove or set to 0 for infinite.
        fadein, fadeout: Number of milliseconds it takes text to appear/disappear. Remove or set 0 to disable.

        offset: Number of screen pixels that the text will be shifted
                Text is shifted based on the [pos] value
            x - number of pixels in horizontal plane
            y - number of pixels in vertical plane


     Examples:

        <string value="FPS %d | Welcome!" color="FF00F265" size="12" family="Tahoma" pos="TopRight" style="Shadowed">
            <showtime value="60000"/>
            <fadein value="500"/>
            <fadeout value="1000"/>
            <msgId value="FPS"/>
        </string>

        <string value="Time is: %s" color="FF00F265" size="12" family="Tahoma" pos="TopRight">
            <showtime value="0"/>
            <msgId value="Time"/>
        </string>

-->
