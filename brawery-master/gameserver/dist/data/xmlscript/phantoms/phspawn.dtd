<?xml version='1.0' encoding='utf-8'?>
<!ELEMENT list (spawn)*>
<!ELEMENT spawn (territory|point|superpoint)*>
<!ATTLIST spawn 
	type CDATA #REQUIRED
	count CDATA #REQUIRED
	gradeMin CDATA #REQUIRED
	gradeMax CDATA #REQUIRED>
<!ELEMENT territory (add)*>
<!ELEMENT add (#PCDATA)>
<!ATTLIST add 
	x CDATA #REQUIRED
	y CDATA #REQUIRED
	zmin CDATA #REQUIRED
	zmax CDATA #REQUIRED>
<!ELEMENT point (#PCDATA)>
<!ATTLIST point
	x CDATA #REQUIRED
	y CDATA #REQUIRED
	z CDATA #REQUIRED>
<!ELEMENT superpoint (step)*>
<!ELEMENT step (delay)*>
<!ATTLIST step
	x CDATA #REQUIRED
	y CDATA #REQUIRED
	z CDATA #REQUIRED>
<!ELEMENT delay (#PCDATA)>
<!ATTLIST delay
	sec CDATA #REQUIRED>